<header id="header">
  <div id="header-content">
    <a href="<%- url_for() %>" id="logo-link">
      <!-- <img id="logo" alt="<%- config.title %>" src="/img/logo.svg"/> -->
      <!-- <svg id="logo" viewBox="0 0 100 100" alt="<%- config.title %>">
        <use href="/img/logo.svg#logo"></use>
      </svg> -->
      <object id="logo" aria-label="UDIA Logo" type="image/svg+xml" data="/img/logo.svg">
        <%- config.title %>
      </object>
    </a>
    <% if (config.subtitle){ %>
      <h2 id="subtitle-wrap">
        <a href="<%- url_for() %>" id="subtitle"><%= config.subtitle %></a>
      </h2>
    <% } %>
    <nav id="main-nav">
      <ul>
        <% for (var i in config.menu){ %>
          <li><a class="main-nav-link" href="<%- url_for(config.menu[i]) %>"><%= i %></a></li>
        <% } %>
      </ul>
    </nav>
    <nav id="sub-nav">
      <% if (theme.rss){ %>
        <a id="nav-rss-link" class="nav-icon" href="<%- url_for(theme.rss) %>" title="<%= __('rss_feed') %>"></a>
      <% } %>
      <a id="nav-search-btn" class="nav-icon" title="<%= __('search') %>"></a>
    </nav>
  </div>
  <div id="theme-selector" style="visibility:hidden;">
    <button type="button" class="select-mode-auto" data-name="auto" title="Set light or dark mode automatically, according to browser setting">Auto</button>
    <button type="button" class="select-mode-light selected" data-name="light" title="Light mode at all times">Light</button>
    <button type="button" class="select-mode-dark" data-name="dark" title="Dark mode at all times">Dark</button>
  </div>
</header>