<header id="header">
  <div id="header-content">
    <a href="<%- url_for() %>" id="logo-link">
      <img id="logo" alt="<%- config.title %>" src="/img/logo.svg"/>
      <!-- <svg id="logo" viewBox="0 0 100 100" alt="<%- config.title %>">
        <use href="/img/logo.svg#logo"></use>
      </svg> -->
    </a>
    <% if (theme.subtitle){ %>
      <h2 id="subtitle-wrap">
        <a href="<%- url_for() %>" id="subtitle"><%= theme.subtitle %></a>
      </h2>
    <% } %>
    <nav id="main-nav">
      <a id="main-nav-toggle" class="nav-icon"></a>
      <% for (var i in theme.menu){ %>
        <a class="main-nav-link" href="<%- url_for(theme.menu[i]) %>"><%= i %></a>
      <% } %>
    </nav>
    <nav id="sub-nav">
      <% if (theme.rss){ %>
        <a id="nav-rss-link" class="nav-icon" href="<%- url_for(theme.rss) %>" title="<%= __('rss_feed') %>"></a>
      <% } %>
      <a id="nav-search-btn" class="nav-icon" title="<%= __('search') %>"></a>
    </nav>
  </div>
</header>